    ViewData["Title"] = "Privacy Policy";
}
<div class="p-3 mb-2 bg-white text-dark">

    <center>
        <div class="icon_1"><img src="images/2.1-removebg-preview (1).png"></div>
        <h4>Sign in to continue</h4>
        <br><br>

        <div class="row mb-3">
            <label for="EMAIL" class="col-sm-2 col-form-label">EMAIL</label>
            <div class="col-sm-10">
                <input type="email" class="form-control" id="EMAIL" placeholder="user@email.com">
            </div>
        </div><br>

        <div class="row mb-3">
            <label for="inputPassword" class="col-sm-2 col-form-label">PASSWORD</label>
            <div class="col-sm-10">
                <input type="password" class="form-control" id="inputPassword3" placeholder="********">
            </div>
        </div><br>

        <button type="button" class="btn btn-dark">login</button>
        <p> new to MoneyMinder? signup</p>

        <button type="button" class="btn btn-dark">signup</button>
    </center>









</div>